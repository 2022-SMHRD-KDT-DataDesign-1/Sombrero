$("input, select").focus(function(e) {
	e.preventDefault();
	$("input, select").parents("li").removeClass("focus");
	$(this).parents("li").addClass("focus");
});

$("input[name='gender']").click(function(e) {
	$("input, select").parents("li").removeClass("focus");
	$("input[name='gender']").parents("li").addClass("focus");
});

$("input, select").focusout(function(e) {
	e.preventDefault();
	$(this).parents("li").removeClass("focus");
});

// 뒤로가기버튼 클릭시
$(window).load(function() {
	if(typeof history.pushState === "function") {
		history.pushState("back", null, null);
		window.onpopstate = function() {
			if (confirm('회원가입을 다음에 진행하시겠습니까?')) {
				//location.href= "http://www.alba.co.kr";
				//MemberCommonCLS.goInOutLink({action:'HOME'});
				var device = $("#device").val();
				var siteui = $("#siteui").val();
				var sitecd = $("#sitecd").val();
				var referurl = document.referrer.toLowerCase();

				if (referurl.indexOf("oauthcallback") > -1 || referurl.indexOf("loginsnsproc") > -1) {
					var url = "/user/join/?site=" + (siteui == "ALBA" ? "MOBILE" : sitecd);
					location.replace(url);
				} else {
					if (device == "") {	//웹일경우는 뒤로가기
						history.back();
					} else {	// 하이브리드앱
						if (siteui == "ALBA" && sitecd == "MOBILE") {	// 알바천국앱일 경우 창닫기
							MemberCommonCLS.goInOutLink({ action: 'CLOSE_WINDOW' });
						} else {	// 나머지앱(채용매니저앱) 뒤로가기
							history.back();
						}
					}
				}
			} else {
				history.pushState("back", null, document.URL);
			}
		};
	}
});

$(document).keydown(function(e) {
	//새로고침 키 방지
	if (e.keyCode == 116) {
		if (confirm("입력된 정보가 사라집니다.\n새로고침 하시겠습니까?")) {
			location.reload();
		} else {
			return false;
		}
	}
});

