<script>
function explorerLayer(type) {
	$('.explorer-wrapper').hide();
	if (type == 'today') {
		CookieCLS.setCookie('explorer_info', '1', 1);
	}
}
(function() {
	if (CookieCLS.getCookie('explorer_info') == "1") {
		$('.explorer-wrapper').hide();
	}
})();
</script>
