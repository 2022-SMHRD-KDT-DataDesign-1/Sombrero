});

//약관 동의 체크
$("#chk0, #chk1, #chk2, #chk3").click(function(e) {
	JoinRegFormPCLS.allAgreeCheck();
});

$("#sectionSms").click(function(e) {
	if ($("#sectionSms").is(":checked")) {
		$("#sectioncd_E").prop("checked", true);
		$("#sectioncd_E").parent().addClass("on");
	} else {
		$("#sectioncd_E").prop("checked", false);
		$("#sectioncd_E").parent().removeClass("on");
	}
	JoinRegFormPCLS.allAgreeCheck();
});

$("#sectionEmail").click(function(e) {
	if ($("#sectionEmail").is(":checked")) {
		$("#sectioncd_A, #sectioncd_B, #sectioncd_I, #sectioncd_C").prop("checked", true);
		$("#sectioncd_A, #sectioncd_B, #sectioncd_I, #sectioncd_C").parent().addClass("on");
	} else {
		$("#sectioncd_A, #sectioncd_B, #sectioncd_I, #sectioncd_C").prop("checked", false);
		$("#sectioncd_A, #sectioncd_B, #sectioncd_I, #sectioncd_C").parent().removeClass("on");
	}
	JoinRegFormPCLS.allAgreeCheck();
});

//모두 동의 체크
$("#allAgree").click(function(e) {
	if ($("#allAgree").is(":checked")) {
		$("#chk0, #chk1, #chk2, #chk3, #sectionSms, #sectionEmail, input[name='sectioncd']").prop("checked", true);
		$("#chk0, #chk1, #chk2, #chk3, #sectionSms, #sectionEmail, input[name='sectioncd']").parent().addClass("on");
	} else {
		$("#chk0, #chk1, #chk2, #chk3, #sectionSms, #sectionEmail, input[name='sectioncd']").prop("checked", false);
		$("#chk0, #chk1, #chk2, #chk3, #sectionSms, #sectionEmail, input[name='sectioncd']").parent().removeClass("on");
	}
});


